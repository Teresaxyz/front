[
  {
    "url":"/main/run",
    "params":{
      "all_nodes":["node_id","type"],
      "all_lines":{
        "line_id":["out_circle_id","in_circle_id"]
      },
      "nodes_details":{
        "node_id":{
          "key":"value"
        }
      },
      "run":["node_name"]
    },
    "return":{"status":"0"}
  },
  {
    "url":"/main/progress",
    "return":{
      "status":"0:complete/1:running",
      "progress":{
        "node_id":"-1:fail/0:succeed/1:running/2:unvisited"
      }
    }
  },
  {
    "url":"/main/sample",
    "params":{
      "number":"<int>",
      "node_name":"node_id"
    },
    "return":{
      "row":"<int>",
      "col":"<int>",
      "index":["col1","col<int>"],
      "data":[["row1col1","row1col<int:col>"],["row<int:row>col1","row<int:row>col<int:col>"]]
    }
  }
]
