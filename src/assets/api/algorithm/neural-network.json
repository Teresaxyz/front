{
  "name":"neural-network",
  "display":"神经网络",
  "inout":"in1out1",
  "attr": [
    {
      "name":"method",
      "display":"方法",
      "type":"list",
      "default":"classify",
      "list":["classify", "regress"]
    },
    {
      "name":"activation",
      "display":"激发函数",
      "type":"list",
      "default":"relu",
      "list":["relu", "identity", "logistic", "tanh"]
    },
    {
      "name":"solver",
      "display":"优化器",
      "type":"list",
      "default":"adam",
      "list":["adam", "lbfgs", "sgd"]
    },
    {
      "name":"alpha",
      "display":"alpha",
      "type":"number",
      "default":"0.0001"
    },
    {
      "name":"label_columns",
      "display":"标签列",
      "type":"text",
      "default":"label"
    }
  ]
}
